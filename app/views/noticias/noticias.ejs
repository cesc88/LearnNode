<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>News</h1>

    <table>
      <tr>
        <td>ID</td>
        <td>Titulo</td>
        <td>Noticias</td>
      </tr>

      <% for(var i=0; i < noticias.length; i++) { %>
          <tr>
              <td><%= noticias[i].id_noticias %></td>
              <td><%= noticias[i].titulo %></td>
              <td><%= noticias[i].noticia %></td>
          </tr>
         <% } %>
    </table>
  </body>
</html>
